
  <table class="table">
    <thead>
      <tr>
        <th>BRAND</th>
        <th>MODEL</th>
        <th>AVAILABILITY</th>
      </tr>
    </thead>
    <tbody>
      <% @cars.each do |car| %>
      <tr>
        <td><%= car.model.brand.brand_name %></td>
        <td><%= car.model.model_name %></td>
        <td>
           <%= car_state(car.state) %>
        </td>
        <% end %>
      </tr>
    </tbody>
  </table>
